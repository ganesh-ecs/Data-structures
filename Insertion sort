#include<stdio.h>
#include<stdlib.h>
int main()
{
    int a[100],n;
    printf("enter the n value:\n");
    scanf("%d",&n);
    printf("enter the array elements:\n");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    int j,temp;
    for(int i=1;i<n;i++)
    {
     temp=a[i];
     j=i-1;
     while(i>0 && a[j]>temp)
     {
        a[j+1]=a[j];
        j--;
     }
     a[j+1]=temp;
    }
    printf("Sorted array is:\n");
    for(int i=0;i<n;i++)
    {
        printf("%d\t",a[i]);
    }
}
